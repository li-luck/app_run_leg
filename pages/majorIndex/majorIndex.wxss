.cus-nav {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  /* padding: 60rpx 0 40rpx 0; */
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
  background-color: #fff;
  font-size: 17px;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 700;
  color: rgba(0, 0, 0, 1);
  z-index: 20;
}

.applet-title {
  width: 320rpx;
  margin: 0 auto;
}

.date-block-padding {
  padding: 0 32rpx;
  position: relative;
  margin-top: -80px;
  background: linear-gradient(180deg, rgb(255, 255, 255, 0%), rgb(255, 255, 255, 100%));
}

.date-search {
  /* width: 100%; */
  border-radius: 32rpx;
  background-color: #fff;
  /* position: absolute; */
  /* margin-top: -80rpx; */
  padding: 40rpx 32rpx 32rpx 32rpx;
  z-index: 10;
  box-shadow: 0 12rpx 48rpx 16rpx rgba(70, 77, 98, 0.03), 0px 5px 14px 0px rgba(70, 77, 98, 0.05), 0 6rpx 16rpx -8rpx rgba(70, 77, 98, 0.08);
}

.shadow {
  position: relative;
  height: 300rpx;
  top: -75rpx;
  z-index: 0;
  background: linear-gradient(180deg, rgb(255, 255, 255, 0%), rgb(255, 255, 255, 100%));
}

.dateCard {
  width: 100%;
  display: flex;
  justify-content: space-between;
  margin-bottom: 31rpx;
}

.dateCardItem {
  height: 100%;
}

.dateCardText {
  margin-bottom: 8rpx;
  color: rgba(38, 51, 65, 0.45);
}

.date-num,
.date-week {
  color: #263341;
}

.dateItemCenter {
  font-size: 26rpx;
  text-align: center;
  align-self: center;
  font-weight: 500;
}

.ml12 {
  margin-left: 12rpx;
}

.search-btn {
  margin: 20rpx auto 20rpx auto;
  width: 600rpx;
  height: 96rpx;
  background: linear-gradient(322deg, #00CCA7 0%, #29CA62 100%);
  border-radius: 16rpx;
  color: #fff;
  line-height: 96rpx;
}

.mt280 {
  margin-top: 180rpx;
}

.content-padding {
  padding: 20rpx;
}

.logo-story {
  width: 112rpx;
  height: 112rpx;
  margin-right: 24rpx;
}

.mt16 {
  margin-top: 16rpx;
}

.s-color {
  color: rgba(38, 51, 65, 0.68);
}

.tag-block {
  justify-content: space-around;
  margin: 96rpx 48rpx 0 48rpx;
  padding: 32rpx 0 32rpx 0;
  background: #FFFFFF;
  box-shadow: 0px 0px 6px 0px rgba(70, 77, 98, 0.1);
  border-radius: 4px;
}

.tag-color {
  color: rgba(38, 51, 65, 0.55);
}

.tag-img {
  width: 48rpx;
  height: 48rpx;
}

.power-block {
  margin-bottom: 100rpx;
}
.power-block_2 {
  margin-bottom: 168rpx;
}

.addMine {
  position: fixed;
  /* top: 148rpx; */
  width: 288rpx;
  padding: 16rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: #FFFFFF;
  background: linear-gradient(136deg, #0093F3 0%, #326FE0 100%);
  box-shadow: 0rpx 32rpx 40rpx 0rpx rgba(50, 111, 224, 0.2);
  border-radius: 8rpx;
  z-index: 30;
}

.addMine::before {
  content: '';
  position: absolute;
  top: -16rpx;
  left: 120rpx;
  width: 0;
  height: 0;
  border-left: 16rpx solid transparent;
  border-right: 16rpx solid transparent;
  border-bottom: 16rpx solid #0093F3;
}

.system-menu {
  width: 48rpx;
  height: 32rpx;
}

.close-addMine {
  width: 32rpx;
  height: 32rpx;
  margin-left: 140rpx;
  padding-left: 14rpx;
  border-left: 2rpx solid rgba(255, 255, 255, 0.3);
}
